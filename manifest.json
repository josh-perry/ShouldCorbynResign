{
    "manifest_version": 2,
    "name": "Should Corbyn Resign?",
    "description": "",
    "version": "0.1",
    "icons": {
    	"16": "Icon-16.png",
    	"48": "Icon-50.png"
    },

    "background": {
    	"scripts": ["background.js"],
    	"persistent": true
    },

    "permissions": [
    ],

    "content_scripts": [
    {
      "matches": [
        "http://news.google.co.uk/*",
        "https://news.google.co.uk/*"
        ],
      "js": ["jquery-2.2.3.min.js", "content.js"],
      "run_at": "document_end"
      }
    ],

    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    }
}
